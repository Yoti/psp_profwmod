+
+static int ReadFile(char*file, void*buff, int size)
+{
+	int ret = 0;
+
+	SceUID fd = sceIoOpen(file, PSP_O_RDONLY, 0777);
+	if (fd < 0)
+		ret = fd;
+	else
+		ret = sceIoRead(fd, buff, size);
+	sceIoClose(fd);
+
+	return ret;
+}
+
+static int WriteFile(char*file, void*buff, int size)
+{
+	int ret = 0;
+
+	sceIoRemove(file);
+	SceUID fd = sceIoOpen(file, PSP_O_WRONLY | PSP_O_CREAT | PSP_O_TRUNC, 0777);
+	if (fd < 0)
+		ret = fd;
+	else
+		ret = sceIoWrite(fd, buff, size);
+	sceIoClose(fd);
+
+	return ret;
+}
