+static int FindFile(char*file)
+{
+	int ret = 0;
+
+	SceUID fd = sceIoOpen(file, PSP_O_RDONLY, 0777);
+		ret = fd;
+	sceIoClose(fd);
+
+	return ret;
+}
+
+static int FileSize(char*file)
+{
+	int ret = 0;
+
+	if (FindFile(file) < 0)
+		ret = -1;
+	else
+	{
+		SceUID fd = sceIoOpen(file, PSP_O_RDONLY, 0777);
+			ret = sceIoLseek(fd, 0, PSP_SEEK_END);
+		sceIoClose(fd);
+	}
+
+	return ret;
